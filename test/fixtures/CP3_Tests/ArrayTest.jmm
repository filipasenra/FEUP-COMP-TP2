import static io.println(int) void;

class ArrayTest{
    int[] array;
    int array_len;

    public int[] get_array() {
        return array;
    }

    public int initialize_array(){
        array = new int[10];
        array_len = array.length();
        return 0;
    }

    public boolean fill_array(){
        array[0] = 1;
        array[1] = 2;
        array[2] = 3;
        array[3] = 4;
        array[4] = 5;
        array[5] = 4;
        array[6] = 3;
        array[7] = 2;
        array[8] = 1;
        array[9] = 0;

        return true;
    }

    public int test_length(){
        int l;
        l = array_len;
        return l;
    }

    public static void main(String[] args){
        ArrayTest array;
        int len;
        int[] arr;

        array = new ArrayTest();
        array.initialize_array();
        array.fill_array();

        len = array.test_length();
        io.println(len);

        arr = array.get_array();
        io.println(arr[0]);
    }
}